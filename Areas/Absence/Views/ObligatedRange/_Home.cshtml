






@{

  var url= Server.MapPath("~/ReportGeneratorOutput/browser/index.html");
  var textHtml = File.ReadAllText(url);

  int firstIndex = textHtml.IndexOf(@"<meta name=""viewport""");
  
  textHtml = 
    textHtml.Substring(0, firstIndex) + // Include the first occurrence
    textHtml.Substring( firstIndex)      // Skip the first occurrence
      .Replace(@"href=""", @"href=""/ReportGeneratorOutput/browser/");
  
  //textHtml=textHtml.Replace(@"href=""", @"href=""/ReportGeneratorOutput/browser/");
  textHtml=textHtml.Replace(@"src=""", @"src=""/ReportGeneratorOutput/browser/");
}

@Html.Raw(textHtml)


@*
