
<h2 style="text-align:center">@ViewBag.Title</h2>
<style>
    .banner * {
        text-align: right;
        direction: rtl;
    }
</style>

<section class="banner bg-circle" id="home">
    <div class="container">
        <div class="row">


            <div class="card">
                <div class="card-body">
                    @foreach (var blog in Model)
                    {
                        <span>
                            @Html.Raw(blog.Content)
                        </span>
                    }
                </div>

            </div>
        </div>
    </div>
</section>

